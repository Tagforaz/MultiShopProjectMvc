@model DetailsCategoryVM
<div class="col-12 grid-margin stretch-card" style="margin-top:60px;">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Details Category</h4>

            <form class="forms-sample" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label>Image</label>
                    <br />
                    <img src="~/assets/img/@Model.Image" style="width:150px; height:150px;" />
                </div>
                <div class="form-group">
                    <label asp-for="Name">Name</label>
                    <input asp-for="Name" style="background-color:gray" ; disabled class="form-control" placeholder="Name">
                    <span asp-validation-for="Name" class="text-danger"></span>
                </div>
           

                <a asp-action="index" class="btn btn-dark">Cancel</a>
            </form>
        </div>
    </div>
</div>